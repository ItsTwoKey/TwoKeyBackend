{
    "builds": [{
        "src": "backend/wsgi.py",
        "use": "@ardnt/vercel-python-wsgi",
        "config": { "maxLambdaSize": "15mb" ,"runtime": "python3.9","output": "dist"}
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "backend/wsgi.py"
        }
    ]
   
        
      
}